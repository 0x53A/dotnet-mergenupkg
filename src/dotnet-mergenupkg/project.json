{
  "version": "0.9.7-*",
  
  "authors": ["Enrico Sada"],
  "summary": "Merge nupkg dependencies",
  "description": "Merge dependencies of two nupkg packages",
  "tags": ["nuget", "nupkg"],
  
  "compilationOptions": {
    "emitEntryPoint": true
  },
  "compilerName": "fsc",
  "compileFiles": [
    "railway.fs",
    "Program.fs"
  ],
  "packInclude": {
    "lib/netstandard1.5/": "fsharpcore/*"
  },
  "dependencies": {
    "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-160316",
    "Microsoft.NETCore.App": {
        "type": "platform",
        "version": "1.0.0-rc2-23925"
    }
  },
  "frameworks": {
    "netstandard1.5": {
      "imports": [
        "portable-net45+win8",
        "dnxcore50"
      ]
    }
  }
}
